{
    "manifest_version": 2,

    "name": "BitBucket Pull Request Review Helper",
    "description": "This extension allows you to flag individual files in a pull request as reviewed",
    "version": "1.1",

    "content_scripts": [
        {
            "matches": ["https://bitbucket.org/*/pull-requests/*"],
            "css": ["style.css"],
            "js": ["index.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ]
}
